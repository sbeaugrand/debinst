@lxpanel --profile LXDE
@pcmanfm --desktop --profile LXDE
@xscreensaver -no-splash
@/usr/bin/numlockx on
@amixer -q set Capture nocap
@xrandr --output eDP --brightness 0.75
@sudo /sbin/modprobe cfg80211
@sudo /sbin/insmod /lib/modules/5.10.0-10-amd64/updates/dkms/8821ce.ko
@nmcli con add type wifi con-name "SSID" ssid "SSID" 802-11-wireless-security.key-mgmt WPA-PSK 802-11-wireless-security.psk "PSK"
@nmcli con up "SSID"
@sudo systemctl start cups
